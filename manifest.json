{
    "manifest_version": 3,
    "name": "holberton school manager tools",
    "description": "a description of the extension",
    "version": "1.0",
    "content_security_policy": {
        "extension_pages": "default-src 'self'"
    },
    "icons": {
        "16": "./icons/hbmt.png",
        "48": "./icons/hbmt.png",
        "128": "./icons/hbmt.png",
        "224": "./icons/hbmt.png"
    },
    "content_scripts": [{
        "matches": ["https://intranet.hbtn.io/*"],
        "run_at": "document_end",
        "js": ["./dist/main.js"]
    }],
    "web_accessible_resources": [{
        "resources": ["./dist/main.js"],
        "matches": ["https://intranet.hbtn.io/*"]
    }]
}